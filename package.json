{
  "name": "ttuacm-website",
  "version": "0.2.0",
  "license": "MIT",
  "description": "ACM TTU Website",
  "scripts": {
    "serve:api": "nodemon --exec firebase serve --only functions",
    "serve:client": "cross-env NODE_PATH=src/client react-app-rewired start",
    "build:client": "cross-env NODE_PATH=src/client react-app-rewired build",
    "test:client": "cross-env NODE_PATH=src/client react-app-rewired test --env=jsdom --testPathPattern=/*/*.spec.jsx",
    "test:unit": "cross-env NODE_ENV=test jest --runInBand --testPathPattern='/src/api/*/*'",
    "test:unit:debug": "cross-env NODE_ENV=test node --inspect node_modules/.bin/jest --runInBand --testPathPattern='/src/api/*/*'",
    "e2e:ci": "cypress run -P ./cypress/staging --record --key $TTUACM_CY_RECORD_KEY",
    "e2e": "cypress open -P ./cypress/development",
    "lint": "eslint src",
    "fmt": "prettier --write 'src/**/*.js'",
    "env-config:client": "cp .env.config .env",
    "env-config:api": "node src/utils/env-config.js",
    "env-save:api": "firebase functions:config:get > .runtimeconfig.json",
    "docs": "apidoc -i src/api/ -o docs/"
  },
  "dependencies": {
    "@firebase/app-types": "0.3.2",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "@material-ui/styles": "3.0.0-alpha.10",
    "axios": "~0.18.0",
    "bcryptjs": "~2.0",
    "body-parser": "~1.18.3",
    "connected-react-router": "4.3",
    "cors": "2.8.4",
    "debug": "3",
    "dotenv": "6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "express": "4.16.3",
    "firebase": "5.5.4",
    "firebase-admin": "6.2.0",
    "firebase-functions": "2.2.0",
    "googleapis": "27",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.12",
    "jsonwebtoken": "8.3.0",
    "jwt-decode": "2.2.0",
    "moment": "2.23.0",
    "mongoose": "5.2.8",
    "nodemailer": "4.6.8",
    "passport": "~0.4.0",
    "passport-facebook": "~2.1.1",
    "passport-github": "~1.1.0",
    "passport-google-oauth2": "~0.1.6",
    "passport-jwt": "~4.0.0",
    "react": "16.8.3",
    "react-app-rewire-hot-loader": "1.0.1",
    "react-app-rewired": "1.5.2",
    "react-dom": "16.8.3",
    "react-firebaseui": "3.1.2",
    "react-hot-loader": "4.3.4",
    "react-redux": "5.1.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.1.4",
    "react-transition-group": "2.4.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "2.3.0"
  },
  "private": true,
  "devDependencies": {
    "@firebase/app-types": "0.3.2",
    "apidoc": "0.17.7",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "cypress": "3.1.5",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "5.9.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-react": "7.11.1",
    "firebase-functions-test": "0.1.5",
    "husky": "1.3.1",
    "jss": "9.8.7",
    "lint-staged": "8.1.4",
    "nodemon": "^1.18.3",
    "prettier": "1.16.4",
    "pretty-format": "24.0.0",
    "react-testing-library": "5.9.0",
    "sinon": "~6.0.0",
    "sinon-chai": "3.2.0",
    "supertest": "3.3.0"
  },
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always",
    "jsxSingleQuote": true
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "setupFiles": [
      "raf/polyfill",
      "./src/client/setupTest.js"
    ],
    "moduleDirectories": [
      "<rootDir>node_modules",
      "<rootDir>src"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write 'src/**/*.js'",
      "git add"
    ]
  },
  "engines": {
    "node": "8"
  },
  "apidoc": {
    "title": "TTUACM API Documentation"
  },
  "peerOptionalDependencies": {
    "kerberos": "1.1.2"
  }
}
